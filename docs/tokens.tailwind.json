      "100": "#FFECC2",
      "200": "#FFD98A",
      "300": "#FFC34D",
      "400": "#FFAE1A",
      "500": "#F59E0B",
      "600": "#D97706",
      "700": "#B45309"
    },
    "warm": {
      "50": "#FAFAF9",
      "100": "#F5F5F4",
