{
  "name": "brand-color-tokens-tailwind",
  "version": "1.0.0",
  "description": "Tailwind-style color scales for Primary (green), Honey (warm secondary), and Warm neutrals.",
  "colors": {
    "primary": {
      "50": "#EAF7EA",
      "100": "#D3F1D3",
      "200": "#B0E6B0",
